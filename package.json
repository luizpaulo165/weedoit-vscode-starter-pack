{
	"name": "weedoit-vscode-starter-pack",
	"displayName": "Weedoit VsCode Starter Pack",
	"description": "A starter pack packeges for VsCode. Indicated for member at Weedo.it company.",
	"version": "0.0.1",
	"icon": "icon.png",
	"publisher":"luizpaulovalenca",
	"engines": {
		"vscode": "^1.38.0"
	},
	"repository": {
		"type": "git", 
		"url": "https://github.com/luizpaulo165/weedoit-vscode-starter-pack",
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"extensionPack": [ 
		"dracula-theme.theme-dracula",
		"ms-vscode.sublime-keybindings",
		"editorconfig.editorconfig",
		"zignd.html-css-class-completion",
		"tsvetan-ganev.nativescript-xml-snippets",
		"nachocab.run-external",
		"eg2.tslint"
    ]
}
